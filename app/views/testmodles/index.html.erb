<p id="notice"><%= notice %></p>

<h1>Listing Testmodles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testmodles.each do |testmodle| %>
      <tr>
        <td><%= testmodle.name %></td>
        <td><%= testmodle.title %></td>
        <td><%= link_to 'Show', testmodle %></td>
        <td><%= link_to 'Edit', edit_testmodle_path(testmodle) %></td>
        <td><%= link_to 'Destroy', testmodle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Testmodle', new_testmodle_path %>
